{
  extends: [
    "config:base",
    "github>suzuki-shunsuke/renovate-config",
    "github>aquaproj/aqua-renovate-config#1.4.0",
  ],
  regexManagers: [
    {
      fileMatch: ["docs/.*\\.md"],
      datasourceTemplate: "github-releases",
      matchStrings: [
        "- uses: (?<depName>[^'\" .@/\\n]+/[^'\" @/\\n]+)@(?<currentValue>.*)",
      ],
    },
  ]
}
